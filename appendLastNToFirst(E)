Node *appendLastNToFirst(Node *head,int n){
    if(head==NULL || n==0){
        return head;
    }
    int count=0;
    Node *temp=head;
    while(temp->next!=NULL){
        count++;
        temp=temp->next;
    }
    
    count++;
    int count1=0;
    Node *temp1=head;
    while(count1<count-n-1){
        temp1=temp1->next;
        count1++;
    }
    Node *newHead=temp1->next;
    temp1->next = NULL;
    temp->next = head;
    return newHead;
}
