Node *midPoint(Node *head){
     if(head==NULL){
         return NULL;
     }
     Node *slow=head;
     Node *fast=head->next;
     while(fast!=NULL && fast->next !=NULL){
         fast=fast->next->next;
         slow=slow->next;
     }
}

APPROACH 2
Node *midPoint(Node *head){
    int count=0;
    Node *temp=head;
    while(temp!=NULL){
        count++;
        temp=temp->next;
    }
 while((count-1)/2){
     head=head->next;
     count-=2;
 }
    return head;
}
